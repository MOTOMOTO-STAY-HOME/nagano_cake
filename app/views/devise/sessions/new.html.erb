  <h4 class="col-xs-offset-2", style="padding:60px 0 30px;"><span style="background-color: #DDDDDD; padding: .3em;">　会員の方はこちらからログイン　</span></h4>

  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <div class="form-group customer-form col-xs-12">
    <%= f.label :email, "メールアドレス", class: "col-xs-2 col-xs-offset-3", style: "text-align: center;" %>
    <%= f.email_field :email, autofocus: true, autocomplete: "メールアドレス", class: "form-inline col-xs-5" %>
  </div>

  <div class="form-group customer-form col-xs-12">
    <%= f.label :password, "パスワード", class: "col-xs-2 col-xs-offset-3", style: "text-align: center;" %>
    <%= f.password_field :password, autofocus: true, autocomplete: "パスワード", class: "form-inline col-xs-5" %>
  </div>

  <div class="col-xs-offset-9">
      <%= link_to "パスワードを忘れた場合はこちら", edit_customer_password_path %>
  </div>

  <%# if devise_mapping.rememberable? %>
    <!--<div class="field">
      <%#= f.check_box :remember_me %>
      <%#= f.label :remember_me %>
    </div>!-->
  <%# end %>

  <div class="form-group">
    <%= f.submit "ログイン", class:"btn btn-primary col-xs-2 col-xs-offset-5" %>
  </div>
  <% end %>

<div class="col-xs-12", style="margin-top: 40px;">
  <h4 class="col-xs-offset-2"><span style="background-color: #DDDDDD; padding: .3em;">　登録がお済でない方　</span></h4>
  <p class="col-xs-offset-3", style="padding-top: 10px;"><%= link_to "こちら", new_customer_registration_path %>　から新規会員登録を行ってください。</p>
</div>



<%#= render "devise/shared/links" %>
